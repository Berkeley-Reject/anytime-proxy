{
    "name": "JobApp Assistant",
    "version": "1.0",
    "description": "Fills out job application form automatically with user-registered data",
    "permissions": ["storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
      
    },
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches" :["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }