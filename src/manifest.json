{
    "name": "Easy Apply",
    "version": "1.0.0",
    "description": "Fills out job application form automatically with user-registered data",
    "icons": {

    },
    "permissions": ["storage"],
    "background": {
      "scripts": ["/js/background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "/html/popup.html"
      
    },
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches" :["<all_urls>"],
        "js": ["/js/content-scripts.js"]
      }
    ]
  }